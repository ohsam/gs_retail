@charset "utf-8";

*{-webkit-text-size-adjust:none;-webkit-box-sizing: border-box;-moz-box-sizing:border-box;box-sizing:border-box}
html{margin:0;width:100%;height:100%}
body{width:100%;height:100%;background:#fff;font-family:AppleSDGothicNeoR00,Apple SD Gothic Neo,MalgunGothic,Dotum,Helvetica,sans-serif;font-size:14px;font-weight:normal;line-height:1.4;color:#222;-webkit-tap-highlight-color:rgba(0,0,0,0.3);word-wrap:break-word;word-break:keep-all;-webkit-overflow-scrolling:touch}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,select,textarea,p,blockquote,th,td,button{margin:0;padding:0}
section,nav,article,aside,hgroup,header,footer,iframe{display:block}
form,fieldset,img,input,button {border:0 none;background:none}
img {margin:0;padding:0;line-height:0;vertical-align:top;width:100%}
em{font-style:normal}
ul,ol,li{list-style:none outside}
a{text-decoration:none;color:#333}
legend{overflow:hidden;display:block;width:0;height:0;text-indent:-9999px}
caption{overflow:hidden;margin:-1px 0 0 -1px;width:1px;height:1px;clip:rect(0 0 0 0)}
.hide, .skip{position:absolute;width:1px;height:1px;margin:-1px;padding:0;overflow:hidden;clip:rect(0,0,0,0);border:0}
input, select, textarea, button{appearance:none;-moz-appearance:none;-webkit-appearance:none;border:0;border-radius:0;background:none;box-shadow:none;outline:0}
input[type='text'],input[type='password'],
input[type='tel'],input[type='number'],
input[type='search'],input[type='email'],
input[type='date']{margin-top:25px;width:100%;height:45px;border-bottom:1px solid #222;font-size:16px}
input[type='search']::-webkit-search-cancel-button{-webkit-appearance:none}
input[type='number']::-webkit-outer-spin-button, input[type=number]::-webkit-inner-spin-button{-webkit-appearance:none}
input[type='date']::-webkit-calendar-picker-indicator {opacity:0}
input[type='date']::-webkit-inner-spin-button {-webkit-appearance:none;display:none}
button, input[type='button'], input[type='submit'], input[type='reset']{cursor:pointer;font:inherit;border-radius:0;-webkit-border-radius:0;-webkit-appearance:none;border:0;vertical-align:middle;background:none}
table{width:100%;border-collapse:collapse;border-spacing:0;table-layout:fixed}

:-ms-input-placeholder{color:#777}
:-moz-placeholder{color:#777}
::-webkit-input-placeholder{color:#777}
textarea:placeholder{color:#777}
input:disabled:-ms-input-placeholder{color:#ccc}
input:disabled:-moz-placeholder{color:#ccc}
input:disabled::-webkit-input-placeholder{color:#ccc}
textarea:placeholder{color:#777}
input:disabled {border-bottom:1px solid #ccc}

[tabindex="-1"]{outline:0;-webkit-tap-highlight-color:rgba(0,0,0,0)}
:disabled{-webkit-tap-highlight-color:rgba(0,0,0,0)}
:-webkit-autofill,
:-webkit-autofill:hover,
:-webkit-autofill:focus,
:-webkit-autofill:active{-webkit-box-shadow:0 0 0 1000px #fff inset;box-shadow:0 0 0 1000px #fff inset}

/** dimmed **/
.dimmed{display:none;z-index:998;position:fixed;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,0)}
/** etc **/
.tr {text-align:right !important}
.tl {text-align:left !important}
.tc {text-align:center !important}
.point {color:#ff0000 !important}
/** layout **/
.skipMenu a{position:fixed;left:0;top:-60px;overflow:hidden;display:block;width:0;height:0;margin-bottom:-1px;background:#00aeef;font-size:16px;font-weight:bold;color:#fff;white-space:nowrap;text-align:center;z-index:10001}
.skipMenu a:focus,
.skipMenu a:active{height:auto;width:100%;color:#fff;line-height:60px}
.skipMenu a:focus,
.skipMenu a:active{top:0}
.wrapper {overflow:hidden;position:relative;width:100%;min-width:320px;min-height:100%}
.content {overflow:hidden;width:100%;padding:60px 0 0}
.header{position:fixed;top:0;left:0;width:100%;height:60px;background:#fff;z-index:99}
.header [class*="btn"] {position:absolute;top:0;height:60px;background:url('../image/common/bg_header.png') 0 0 no-repeat;background-size:148px 60px;text-indent:-9999px}
.header .btnBack{left:0;width:68px;background-position:0 0}
.header .btnQr{right:0;width:80px;background-position:-68px 0}
.header h1{overflow:hidden;width:100%;padding:0 80px 0 68px;font-size:20px;text-align:center;line-height:60px;white-space:nowrap;text-overflow:ellipsis}
@media screen and (max-width: 320px){
	.header h1{font-size:16px}
}
/* quickMenu */
.MnQuick [class*="quick"] {display:block;width:50px;height:50px;background:url(../image/common/bg_ico_quick.png) 0 0 no-repeat;background-size:50px auto;text-indent:-9999px}
.MnQuick .quickBack {position:fixed;left:25px;bottom:25px;z-index:9999}
.MnQuick .quickTop {background-position:0 -50px}
.MnQuick .quickGsr {margin-top:10px;background-position:0 -100px}
.rightQuick {position:fixed;right:25px;bottom:25px;z-index:9999}
.rightQuick a {border-radius:50%;box-shadow:15px 15px 23px -18px rgba(0,0,0,1)}

/** paragraph **/
.subTitdep01 {padding:40px 0 25px;font-size:18px;line-height:25px}
.subTitdep01.type2{padding:40px 0 15px}

/** tab **/
.tabArea {/*position:relative*/position:fixed;top:60px;left:0;width:100%;background:#fff;z-index:6}
.tabArea ul{overflow:auto;padding:0 14px;border-bottom:1px solid #e9e9e9;white-space:nowrap}
.tabArea ul::-webkit-scrollbar{display:none}
.tabArea ul:after{content:'';display:block;position:absolute;top:0;right:-12px;bottom:1px;width:50px;z-index:1;
	background: -moz-linear-gradient(left, rgba(255,255,255,0) 0%, rgba(255,255,255,1) 100%); 
	background: -webkit-linear-gradient(left, rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%); 
	background: linear-gradient(to right, rgba(255,255,255,0) 0%, rgba(255,255,255,1) 100%); 
}
.tabArea ul li {display:inline-block;margin:0 -2px}
.tabArea ul li a{display:block;position:relative;padding:22px 12px 16px;font-size:17px;font-weight:bold;text-align:center;line-height:1}
.tabArea ul li a[aria-selected="true"] {padding-bottom:0}
.tabArea ul li a[aria-selected="true"]:after {content:'';display:block;width:100%;height:2px;margin-top:14px;background:#fb5169}

.tabTog{padding:12px 25px;background:#f2f2f2}
.tabTog ul {display:flex;flex-wrap:wrap;height:35px;background:#fff;border-radius:35px}
.tabTog ul li{flex:1 auto}
.tabTog ul li a{display:block;padding-top:2px;color:#222;font-size:16px;line-height:33px;text-align:center}
.tabTog ul li a[aria-selected="true"]{color:#fff;background:#fb5169;border-radius:35px}

.tabCont {padding-top:55px}
.infoTxt {padding:15px 25px;background:#f2f2f2;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;font-size:16px;line-height:23px}
/** coretext **/
.coreTxt {padding:30px 0 40px;text-align:center} 
.coreTxt strong {font-size:22px;font-weight:normal}
.coreTxt p {padding-top:15px;font-size:14px;color:#777;line-height:20px}

/** component 
.component{position:relative;padding:8px 25px 0}
.component:before{content:'';display:block;position:absolute;top:0;left:0;width:100%;height:8px;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;background:#f2f2f2}
.component:first-child{padding-top:30px}
.component:first-child:before{display:none}**/
/* 단 구분 라인 */
.line {margin-top:10px;position:relative}
.line:before{content:'';position:absolute;top:-10px;left:0;width:100%;height:8px;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;background:#f2f2f2}

/** noti **/
.notiInfo{padding:40px 25px;background:#fff;border-top:1px solid #ddd}
.notiInfo dt{padding-bottom:10px;font-size:16px}
.notiInfo dd,
.notiInfo.type02 li,
ul.bl li,
p.bl{position:relative;padding:5px 0 0 6px;color:#777;line-height:22px}
.notiInfo dd:before,
.notiInfo.type02 li:before,
ul.bl li:before,
p.bl:before{content:'';display:block;position:absolute;top:15px;left:0;width:2px;height:2px;border-radius:50%;background:#777}
.notiInfo+.prbtnFull{margin-top:0}
.notiInfo+.prbtnFull:before{background: -moz-linear-gradient(top, rgba(245,245,245,0.8) 0%, rgba(224,224,224,0.64) 100%);
	background: -webkit-linear-gradient(top, rgba(245,245,245,0.8) 0%, rgba(224,224,224,0.64) 100%);
	background: linear-gradient(to bottom, rgba(245,245,245,0.8) 0%, rgba(224,224,224,0.64) 100%)}
.notiInfo.type02{padding:35px 25px 40px}
.notiInfo a {color:#777;text-decoration:underline}

/** form **/
/* radio,check */
.checkForm {position:relative;display:inline-block;/*padding-left:33px;*/z-index:2}
.checkForm input[type='checkbox'], 
.checkForm input[type='radio']{position:absolute;top:0;left:0;width:23px;height:23px;margin-top:0;border:0;opacity:0}
.checkForm input[type='checkbox']+label, 
.checkForm input[type='radio']+label{position:relative;display:inline-block;max-width:100%;padding:3px 0 0 34px;/*text-indent:-33px*/}
.checkForm input[type='checkbox']+label:before, 
.checkForm input[type='radio']+label:before{content:'';/*display:inline-block;*/position:absolute;top:0;left:0;width:23px;height:23px;/*margin-right:10px;*/background:url(../image/common/bg_form.png) no-repeat 0 0;background-size:23px auto;/*vertical-align:middle*/vertical-align:-6px}
.checkForm input[type='radio']+label:before {background-position:0 -46px}
.checkForm input[type='checkbox']:checked+label:before{background-position:0 -23px} 
.checkForm input[type='radio']:checked+label:before{background-position:0 -69px}
/*.checkForm input[type='checkbox']:focus+label:before, 
.checkForm input[type='radio']:focus+label:before{width:21px;height:21px;border:1px solid #808080}*/
/* toggle swich */
.tgSwich{display:inline-block;position:relative}
.tgSwich input{position:absolute;top:0;left:0;width:100%;height:100%;margin-top:0;outline:0;border:0}
.tgSwich label{display:block;position:relative;z-index:2;width:48px;height:26px;border-radius:17px;background:#a5a5a5;transition:.4s}
.tgSwich label:before{content:'';position:absolute;top:2px;left:2px;width:22px;height:22px;border-radius:50%;background:#fff;box-shadow:0 2px 1px rgba(0,0,0,0.2);transition:.4s}
.tgSwich input:checked + label{background:#fa5169}
.tgSwich input:checked + label:before {transform:translateX(100%)}

.frmButton{display:flex;flex-wrap:nowrap;margin-left:-5px}
.frmButton > span{position:relative;flex:0 20%;padding-left:5px}
.frmButton input{position:absolute;top:0;left:5px;right:0;height:100%;border:0;border-radius:0;background:#fff;-webkit-appearance:none;appearance:none}
.frmButton label{position:relative;display:block;height:35px;color:#777;font-size:15px;line-height:32px;text-align:center;border:1px solid #ccc;border-radius:3px;box-sizing:border-box}
.frmButton input:checked+label{color:#fff;border-color:#fa5169;background:#fa5169}
/* select */
select {position:relative;width:100%;height:45px;font-size:16px;border:none;border-bottom:1px solid #222;background:url(../image/common/bg_form.png) no-repeat 100% -92px;background-size:23px auto}

.setSorting{display:flex;flexw-rap;border-top:1px solid #eee}
.setSorting .sorting{position:relative;flex:1 50%;border:0;background:none;text-align:center}
.setSorting .sorting select{position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;border:0;background:none;z-index:1}
.setSorting .sorting option{color:#222}
.setSorting .sorting label:before{content:'';display:inline-block;width:15px;height:15px;margin:-2px 10px 0 0;vertical-align:middle;background:url(../image/common/bg_sorting.png) 0 0 no-repeat;background-size:100% auto}
.setSorting .sorting label{display:inline-block;font-size:16px;line-height:47px}
.setSorting .sorting.filter label:before{background-position:0 0}
.setSorting .sorting.array label:before{background-position:0 -15px}
.setSorting .sorting+.sorting:before{content:'';position:absolute;top:50%;left:0;width:1px;height:20px;margin-top:-10px;background:#eee}
/* action input */
.activeInput {position:relative;display:block;margin-top:25px}
.activeInput.disabled label{color:#ccc}
.activeInput label {position:absolute;top:13px;left:0;font-size:16px;color:#777}
.activeInput.error label,
.activeInput.noInput label{color:#f63939}
.activeInput input {margin-top:0;padding-right:40px}
.activeInput input.trInput {padding-right:25px;text-align:right}
.activeInput input.crnumInput {padding-right:75px}
.activeInput b {position:absolute;top:13px;right:0;font-size:16px;color:#111}
.activeInput b.time {display:none;color:#ff0000}
.activeInput button[type="button"] {display:none;position:absolute;top:50%;right:0;width:18px;height:18px;margin-top:-9px;background:url(../image/common/bg_form.png) no-repeat 0 -137px;background-size:23px auto;text-indent:-9999px}
.activeInput.action label {top:-7px;font-size:13px}
.activeInput.action label em {display:none}
.activeInput.action input.trInput {padding-right:45px}
.activeInput.action b {right:27px}
.activeInput.action b.time,
.activeInput.action button {display:block}
/* 검색 */
.srch {margin-top:0}
.srch input {padding-right:80px;height:52px;border-bottom:1px solid #777;font-size:16px;color:#777}
.srch button[type="submit"] {position:absolute;top:50%;right:0;width:53px;height:45px;margin-top:-23px;background:url('../image/common/bg_ico_search.png') 0 0 no-repeat;background-size:53px 45px;text-indent:-9999px}
.srch button[type="button"] {right:50px}
.inputArea {padding:15px;background:#f2f2f2}
.inputArea input {padding-left:15px;background:#fff;border-bottom:none;border-radius:5px}
/* 글자 수 제한사항 안내 */
.activeInput input.lengInput{color:#777}
.notiLength{margin-top:12px;font-size:14px}
.notiLength:after{content:'';display:block;clear:both}
.notiLength span{float:left;color:#777}
.notiLength span+span{float:right}
/* 오류 안내 */
.activeInput.action+.notiError{display:none}
.notiError{margin-top:12px;font-size:14px;color:#f63939}
.notiError li{position:relative;padding-left:8px}
.notiError li:before{content:'';position:absolute;top:8px;left:0;width:2px;height:2px;background:#f63939}
.notiError li+li{margin-top:6px}
/* inputSel */
.subTitdep01+.inputSel{margin-top:0}
.inputSel{margin-top:25px}
.inputSel .activeInput:first-child,
.inputSel .activeInput:last-child{display:inline-block;margin-top:0;width:calc(100% - 125px);width:-webkit-calc(100% - 125px);vertical-align:bottom}
.inputSel .activeInput:first-child{margin-right:5px}
.inputSel .activeInput:last-child{margin-left:5px}
.inputSel select,
.inputSel [class*="btnT"] {width:115px}
.notiTxt{margin-top:15px;padding-left:11px;font-size:14px;color:#777;text-indent:-11px}
/* 자동 로그인 */
.autoLogin{position:relative;margin-top:40px;padding:22px 15px;background:#f5f5f5}
.autoLogin .txt {font-size:16px;line-height:1}
.autoLogin .tgSwich {position:absolute;top:19px;right:15px}
/* date input */
input[type='date'] {width:100%;border-bottom:1px solid #222;height:45px;background:url('../image/common/bg_form.png') 100% -169px no-repeat;background-size:23px auto;font-size:16px;color:#777}
.selectSel{margin-top:5px}
.selectSel:after{content:'';display:block;clear:both}
.selectSel input,
.selectSel select {float:left;width:calc(50% - 10px);width:-webkit-calc(50% - 10px)}
.selectSel input+input,
.selectSel select+select {float:right}
/* textForm */
.textForm{overflow-y:scroll;height:83px;padding:15px;border:1px solid #ddd;font-size:16px;background:#fff}


/** accordian **/
.acc {border-top:1px solid #d5d5d5}
.acc>li{border-bottom:1px solid #d5d5d5}
.acc .accTit {position:relative;width:100%;background:#fff}
.acc .accTit.on {border-bottom:none}
.acc .accTit button {width:100%;padding:20px 50px 20px 25px;font-size:16px;font-weight:normal;text-align:left;vertical-align:middle}
.acc .accTit button:after {content:'';display:block;position:absolute;top:50%;right:25px;width:15px;height:15px;margin-top:-7px;background:url(../image/common/bg_ico_acc.png) 0 0 no-repeat;background-size:15px auto}
.acc .accTit.on button:after {background-position:0 -15px}
.acc .accCon {display:none;padding:25px;background:#f5f5f5;font-size:16px}
.acc .accTit.on+.accCon {display:block}
.acc .accCon {padding-bottom:45px}
.acc .accTit.type02 {padding:20px 50px 20px 25px}
.acc .accTit.type02>* {position:relative;z-index:2}
.acc .accTit.type02 button {position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;font-size:0;line-height:0;z-index:1}

/** FAQ **/
.acc.faq .accTit+.accCon{position:relative;padding:25px 25px 25px 47px;color:#555}
.acc.faq .accTit+.accCon:before{content:'A';display:inline-block;position:absolute;top:25px;left:25px;font-size:20px;color:#fa5169;font-weight:bold}

/** button **/
[class^="btnT"]{display:inline-block;padding:1px 14px 0;color:#222;border:1px solid #ccc;background:#fff;text-align:center}
[class^="btnT"].active{color:#fff;border-color:#fa5169;background:#fa5169}
[class^="btnT"].color{color:#fa5169;border-color:#fa5169}
[class^="btnT"].arr:after{content:'';display:inline-block;width:9px;height:9px;margin:0 2px 1px 2px;border:solid #777;border-width:1px 1px 0 0;transform:rotate(45deg)}
[class^="btnT"]:disabled, [class^="btnT"][aria-disabled="true"]{color:rgba(255,255,255,.4)!important;border-color:#a5a5a5!important;background:#a5a5a5!important}
.btnTS{border-radius:3px}
.btnL{height:50px;font-size:18px;line-height:47px}
.btnM{height:40px;font-size:16px;line-height:37px}
.btnS{height:33px;padding:1px 10px 0;font-size:14px;line-height:30px}

.btnMore{display:block;height:50px;font-size:18px;line-height:48px;color:#fff;border-color:#fa5169;border-radius:3px;background:#fa5169}
.btnMore em:before{content:'';display:inline-block;width:15px;height:15px;vertical-align:-1px;margin:0 5px 0 10px;background:url(../image/common/bg_ico_more.png) 0 0 no-repeat;background-size:15px}

.btnView{display:inline-block;color:#fb5169;font-size:16px;line-height:1;border-bottom:1px solid #fb5169}

.btnArea{display:flex;flex-wrap:wrap;padding:25px}
.btnArea a, .btnArea button{flex:1 20%;margin:0 6px}
.btnArea a:first-child, .btnArea button:first-child{margin-left:0}
.btnArea a:last-child, .btnArea button:last-child{margin-right:0}

.btnTP{color:#fff;font-size:18px;border-color:#555;background:#555}
.btnPrss{display:flex;flex-wrap:wrap;margin:25px;overflow:hidden}
.btnPrss .btnTP{flex:1 45%;height:55px;margin:0 5px;border-radius:3px}
.btnPrss .btnTP:first-child{margin-left:0}
.btnPrss .btnTP:last-child{margin-right:0}
.btnFixed{height:60px}
.btnFixed .btnPrss{position:fixed;bottom:0;left:0;width:100%;margin:0;box-shadow:10px 0 20px rgba(0,0,0,.1);z-index:5}
.btnFixed .btnPrss .btnTP{flex:1 50%;height:60px;margin:0;border-radius:0}

.btnFixed.prFixed{height:115px}
.btnFixed.prFixed .prTotal{flex-basis:100%;height:55px;background:#fff}

/** table **/
.infoArea .date{margin:40px 0 10px;font-size:14px;line-height:1;color:#777;text-align:right}
[class*="tbl"] {overflow:hidden;width:100%;border-top:1px solid #222;border-bottom:1px solid #ccc;text-align:left;background:#fff} 
[class*="tbl"] th,
[class*="tbl"] td{padding:20px 0 20px 20px;font-size:16px}
[class*="tbl"] th{color:#222;font-weight:normal}
[class*="tbl"] td{color:#555}
[class*="tbl"] thead th{font-weight:bold}
[class*="tbl"] thead th,
.tblV tbody th{background:#f5f5f5}
[class*="tbl"] tbody tr+tr th,
[class*="tbl"] tbody tr+tr td {border-top:1px solid #eee}

/** swipe **/
.swipeArea .swipeCont{position:relative}
.swipeArea .swipeCont .swiper{display:flex;flex-wrap:nowrap}
.swipeArea .swipeCont .swiper>li{flex:1 0 100%}
.swipeArea .swipeCont .swiper>li a{display:block}
.swipeArea .indicator{display:block;margin-top:25px;line-height:9px;text-align:center}
.swipeArea .indicator button{width:5px;height:5px;margin:0 2px;text-indent:-9999px;border-radius:5px;background:#ccc;outline:4px transparent}
.swipeArea .indicator button[aria-selected="true"]{width:18px}

/** nodata **/
.noData{padding:70px 0;text-align:center}
.noData .desc{color:#777;font-size:16px}
.noData .desc:before{content:'';display:block;width:75px;height:75px;margin:0 auto 25px;background:url(../image/common/bg_nodata.png) 0 0 no-repeat;background-size:100% auto}
.noData .desc.type02:before{background:rgba(0,0,255,.5)}
.noData .desc.type03:before{background:rgba(0,255,0,.5)}
.noData .link{margin-top:20px}
.noData .link li{padding-top:25px}

/** full Popup **/
.fullPop{position:absolute;top:0;left:0;width:100%;height:100%;background:#fff;z-index:9999}
.fullPop h1{position:fixed;padding:0 58px;width:100%;height:60px;background:#fff;line-height:60px;font-size:20px;font-weight:bold;text-align:center;z-index:1;white-space:nowrap;text-overflow:ellipsis;overflow:hidden}
.fullPop .popCon{margin-top:60px;overflow-y:auto}
.fullPop .btnClose{position:fixed;top:0;right:0;width:58px;height:60px;background:url(../image/common/btn_popClose.png) 0 0 no-repeat;background-size:58px auto;text-indent:-9999px;z-index:1}

/** 약관동의 **/
