@charset "utf-8";

*{-webkit-text-size-adjust:none;-webkit-box-sizing: border-box;-moz-box-sizing:border-box;box-sizing:border-box}
html{margin:0;width:100%;height:100%}
body{height:100%;background:#fff;font-family:AppleSDGothicNeoR00,Apple SD Gothic Neo,MalgunGothic,Dotum,Helvetica,sans-serif;font-size:14px;line-height:20px;color:#222;-webkit-tap-highlight-color:rgba(0,0,0,0.3);word-wrap:break-word;word-break:keep-all;-webkit-overflow-scrolling:touch}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,select,textarea,p,blockquote,th,td,button{margin:0;padding:0}
section,nav,article,aside,hgroup,header,footer,iframe{display:block}
form,fieldset,img,input,button {border:0 none;background:none}
img {margin:0;padding:0;line-height:0;vertical-align:top;width:100%}
em{font-style:normal}
ul,ol,li{list-style:none outside}
a{text-decoration:none;color:#333}
legend{overflow:hidden;display:block;width:0;height:0;text-indent:-9999px}
caption{overflow:hidden;margin:-1px 0 0 -1px;width:1px;height:1px;clip:rect(0 0 0 0)}
.hide, .skip{position:absolute;width:1px;height:1px;margin:-1px;padding:0;overflow:hidden;clip:rect(0,0,0,0);border:0}
input[type='text'],input[type='password'],
input[type='tel'],input[type='number'],
input[type='search'],input[type='email'],
input[type='date']{-webkit-border-radius:0;border-radius:0;-webkit-appearance:none;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;outline:none}
input[type='search']::-webkit-search-cancel-button{-webkit-appearance:none}
input[type='number']::-webkit-outer-spin-button, input[type=number]::-webkit-inner-spin-button{-webkit-appearance:none}
input[type='date']::-webkit-calendar-picker-indicator {opacity:0}
input[type='date']::-webkit-inner-spin-button {-webkit-appearance:none;display:none}
button, input[type='button'], input[type='submit'], input[type='reset']{cursor:pointer;border-radius:0;-webkit-border-radius:0;-webkit-appearance:none;border:0;vertical-align:middle;background:none}
table{width:100%;border-collapse:collapse;border-spacing:0;table-layout:fixed}
input{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;-webkit-border-radius:0;-webkit-appearance:none;vertical-align:middle}
/** dimmed **/
.dimmed{display:none;z-index:998;position:fixed;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,0.8)}
/** etc **/
.tr {text-align:right !important}
.tl {text-align:left !important}
.tc {text-align:center !important}
.point {color:#ff0000 !important}
/** layout **/
.skipMenu a{position:fixed;left:0;top:-60px;overflow:hidden;display:block;width:0;height:0;margin-bottom:-1px;background:#00aeef;font-size:16px;font-weight:bold;color:#fff;white-space:nowrap;text-align:center;z-index:10001}
.skipMenu a:focus,
.skipMenu a:active{height:auto;width:100%;color:#fff;line-height:60px}
.skipMenu a:focus,
.skipMenu a:active{top:0}
.wrapper {overflow:hidden;position:relative;width:100%;min-width:320px;min-height:100%}
.content {overflow:hidden;width:100%;padding:60px 0 0}
.header{position:fixed;width:100%;height:60px;background:#fff;z-index:99}
.header [class*="btn"] {position:absolute;top:0;height:65px;background:url('../image/common/bg_header.png') 0 0 no-repeat;background-size:148px 60px;text-indent:-9999px}
.header .btnBack{left:0;width:68px;background-position:0 0}
.header .btnQr{right:0;width:80px;background-position:-68px 0}
.header h1{overflow:hidden;width:100%;padding:0 80px 0 68px;font-size:20px;text-align:center;line-height:60px;white-space:nowrap;text-overflow:ellipsis}
@media screen and (max-width: 320px){
	.header h1{font-size:16px}
}
/* quickMenu */
.MnQuick [class*="quick"] {display:block;width:50px;height:50px;background:url(../image/common/bg_ico_quick.png) 0 0 no-repeat;background-size:50px auto;text-indent:-9999px}
.MnQuick .quickBack {position:fixed;left:25px;bottom:25px;z-index:9999}
.MnQuick .quickTop {background-position:0 -50px}
.MnQuick .quickGsr {margin-top:10px;background-position:0 -100px}
.rightQuick {position:fixed;right:25px;bottom:25px;z-index:9999}
.rightQuick a {border-radius:50%;box-shadow:15px 15px 23px -18px rgba(0,0,0,1)}

/** paragraph **/
.subTitdep01 {padding:40px 0 25px;font-size:18px;line-height:25px}
.subTitdep01.type2{padding:40px 0 15px}

/** tab **/
.tabArea {position:relative}
.tabArea:after{content:'';display:block;position:absolute;top:0;right:-12px;width:50px;height:57px;z-index:1;
	background: -moz-linear-gradient(left, rgba(255,255,255,0) 0%, rgba(255,255,255,1) 100%); 
	background: -webkit-linear-gradient(left, rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%); 
	background: linear-gradient(to right, rgba(255,255,255,0) 0%, rgba(255,255,255,1) 100%); 
}
.tabArea ul{overflow:auto;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;padding:0 12px;border-bottom:1px solid #e9e9e9}
.tabArea ul::-webkit-scrollbar{display:none}
.tabArea ul li {-webkit-flex:0 auto;flex:0 auto}
.tabArea ul li a{display:block;position:relative;padding:20px 12px 16px;font-size:17px;font-weight:bold;text-align:center}
.tabArea ul li a[aria-selected="true"] {padding-bottom:0}
.tabArea ul li a[aria-selected="true"]:after {content:'';display:block;width:100%;height:2px;margin-top:15px;background:#fb5169}

.tabTog{padding:12px 25px;background:#f2f2f2}
.tabTog ul {display:flex;flex-wrap:wrap;height:35px;background:#fff;border-radius:35px}
.tabTog ul li{flex:1 auto}
.tabTog ul li a{display:block;color:#222;font-size:16px;line-height:35px;text-align:center}
.tabTog ul li a[aria-selected="true"]{color:#fff;font-weight:bold;background:#fb5169;border-radius:35px}

/** coretext **/
.coreTxt {padding:30px 0 40px;text-align:center} 
.coreTxt strong {font-size:22px;font-weight:normal}
.coreTxt p {padding-top:15px;font-size:14px;color:#777;line-height:20px}

/** component **/
.component{position:relative;padding:8px 25px 0}
.component:before{content:'';display:block;position:absolute;top:0;left:0;width:100%;height:8px;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;background:#f2f2f2}
/*.component:first-child{padding-top:30px}
.component:first-child:before{display:none}*/
/** 단 구분 라인 
.line{height:8px;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;background:#f2f2f2}**/

/** noti **/
.notiInfo{padding:40px 25px 15px;background:#f5f5f5}
.notiInfo dt{padding-bottom:10px;font-size:16px}
.notiInfo dd,
.notiInfo.type02 li,
ul.bl li,
p.bl{position:relative;padding:5px 0 0 6px;color:#777;line-height:22px}
.notiInfo dd:before,
.notiInfo.type02 li:before,
ul.bl li:before,
p.bl:before{content:'';display:block;position:absolute;top:15px;left:0;width:2px;height:2px;border-radius:50%;background:#777}
.notiInfo+.prbtnFull{margin-top:0}
.notiInfo+.prbtnFull:before{background: -moz-linear-gradient(top, rgba(245,245,245,0.8) 0%, rgba(224,224,224,0.64) 100%);
	background: -webkit-linear-gradient(top, rgba(245,245,245,0.8) 0%, rgba(224,224,224,0.64) 100%);
	background: linear-gradient(to bottom, rgba(245,245,245,0.8) 0%, rgba(224,224,224,0.64) 100%)}
.notiInfo.type02{padding:35px 25px 40px;}


/** form **/
/* radio,check */
.checkForm {position:relative;display:inline-block;padding-left:33px;z-index:2}
.checkForm input[type='checkbox'], 
.checkForm input[type='radio']{position:absolute;top:0;left:0;width:23px;height:23px;opacity:0}
.checkForm input[type='checkbox']+label, 
.checkForm input[type='radio']+label{position:relative;display:inline-block;max-width:100%;text-indent:-33px}
.checkForm input[type='checkbox']+label:before, 
.checkForm input[type='radio']+label:before{content:'';display:inline-block;width:23px;height:23px;margin-right:10px;background:url(../image/common/bg_form.png) no-repeat 0 0;background-size:23px auto;/*vertical-align:middle*/vertical-align:-6px}
.checkForm input[type='radio']+label:before {background-position:0 -46px}
.checkForm input[type='checkbox']:checked+label:before{background-position:0 -23px} 
.checkForm input[type='radio']:checked+label:before{background-position:0 -69px}
/*.checkForm input[type='checkbox']:focus+label:before, 
.checkForm input[type='radio']:focus+label:before{width:21px;height:21px;border:1px solid #808080}*/
/* toggle swich */
.tgSwich{display:inline-block;position:relative}
.tgSwich input{position:absolute;top:0;left:0;width:100%;height:100%;outline:0}
.tgSwich label{display:block;position:relative;z-index:2;width:48px;height:26px;border-radius:17px;background:#a5a5a5;transition:.4s}
.tgSwich label:before{content:'';position:absolute;top:2px;left:2px;width:22px;height:22px;border-radius:50%;background:#fff;box-shadow:0 2px 1px rgba(0,0,0,0.2);transition:.4s}
.tgSwich input:checked + label{background:#fa5169}
.tgSwich input:checked + label:before {transform:translateX(100%)}

/* select */
select {position:relative;width:100%;height:45px;font-size:16px;font-weight:600;border:none;border-bottom:1px solid #222;background:url(../image/common/bg_form.png) no-repeat 100% -92px;background-size:23px auto;-webkit-appearance:none;-moz-appearance:none;appearance:none}
/* action input */
.activeInput {position:relative;display:block;margin-top:24px}
.activeInput.disabled label{color:#ccc;}
.activeInput.disabled input{border-bottom: 1px solid #ccc;}
.activeInput label {position:absolute;top:15px;left:0;font-size:16px;color:#777}
.activeInput.error label,
.activeInput.noInput label{color:#fa5169}
.activeInput input {width:100%;height:45px;padding-top:10px;padding-right:40px;border-bottom:1px solid #222;font-size:16px}
.activeInput input.trInput {padding-right:25px;text-align:right}
.activeInput input.crnumInput {padding-right:75px}
.activeInput b {position:absolute;top:16px;right:0;font-size:16px;color:#111}
.activeInput b.time {display:none;color:#ff0000}
.activeInput button {display:none;position:absolute;top:16px;right:0;width:18px;height:18px;background:url(../image/common/bg_form.png) no-repeat 0 -137px;background-size:23px auto;text-indent:-9999px}
.activeInput.action label {top:-4px;font-size:13px}
.activeInput.action label em {display:none}
.activeInput.action input.trInput {padding-right:45px}
.activeInput.action b {right:27px}
.activeInput.action b.time,
.activeInput.action button {display:block}

/* 글자 수 제한사항 안내 */
.activeInput input.lengInput{color:#777;}
.notiLength{margin-top:10px;font-size:13px}
.notiLength:after{content:'';display:block;clear:both}
.notiLength span{float:left;color:#f8793a}
.notiLength span+span{float:right;color:#777}

/* inputSel */
.subTitdep01+.inputSel{margin-top:0}
.inputSel{margin-top:24px}
.inputSel .activeInput:first-child,
.inputSel .activeInput:last-child{display:inline-block;margin-top:0;width:calc(100% - 125px);width:-webkit-calc(100% - 125px);vertical-align:bottom}
.inputSel .activeInput:first-child{margin-right:5px}
.inputSel .activeInput:last-child{margin-left:5px}
.inputSel select,
.inputSel [class*="Btn"] {width:115px;}
.notiTxt{margin-top:15px;padding-left:11px;font-size:14px;color:#777;line-height:19px;text-indent:-11px}
@media screen and (max-width: 320px){
.activeInput label em {display:none}
}
/* 자동 로그인 */
.autoLogin{position:relative;margin-top:40px;padding:22px 15px;background:#f5f5f5}
.autoLogin .txt {font-size:16px;line-height:16px}
.autoLogin .tgSwich {position:absolute;top:19px;right:15px}
/* date input */
input[type='date'] {width:100%;border-bottom:1px solid #222;height:45px;background:url('../image/common/bg_form.png') 100% -169px no-repeat;background-size:23px auto;font-size:16px;color:#777}
.selectSel{margin-top:5px}
.selectSel:after{content:'';display:block;clear:both}
.selectSel input,
.selectSel select {float:left;width:calc(50% - 10px);width:-webkit-calc(50% - 10px)}
.selectSel input+input,
.selectSel select+select {float:right}
/* textForm */
.textForm{overflow-y:scroll;height:83px;padding:15px;border:1px solid #ddd;font-size:16px;line-height:21px;background:#fff}
/* 검색 */
.srch {position:relative;margin-bottom:30px}
.srch input {width:100%;padding-right:25px;height:52px;border-bottom:1px solid #777;font-size:16px;color:#777}
.srch button {position:absolute;top:0;right:0;width:18px;height:52px;background:url('../image/common/bg_form.png') 0 -214px no-repeat;background-size:23px auto;text-indent:-9999px}

/** accordian **/
.acc {border-top:1px solid #d5d5d5}
.acc .accTit {position:relative;display:table;width:100%;background:#fff;border-bottom:1px solid #d5d5d5}
.acc .accTit.on {border-bottom:none}
.acc .accTit button {display:table-cell;width:100%;padding:20px 50px 20px 25px;font-size:16px;font-weight:normal;text-align:left;vertical-align:middle;line-height:25px}
.acc .accTit button:after {content:'';display:block;position:absolute;top:50%;right:25px;width:15px;height:15px;margin-top:-7px;background:url(../image/common/bg_ico_acc.png) 0 0 no-repeat;background-size:15px auto}
.acc .accTit.on button:after {background-position:0 -15px}
.acc .accTit+.accCon {display:none;padding:25px;border-bottom:1px solid #d5d5d5;background:#f5f5f5;font-size:16px}
.acc .accTit.on+.accCon {display:block}
.acc .accCon {padding-bottom:45px}
.acc .accTit.type02 {padding:20px 50px 20px 25px}
.acc .accTit.type02>* {position:relative;z-index:2}
.acc .accTit.type02 button {position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;font-size:0;line-height:0;z-index:1}

/** FAQ **/
.acc.faq .accTit+.accCon{position:relative;padding:25px 25px 25px 47px;color:#555}
.acc.faq .accTit+.accCon:before{content:'A';display:inline-block;position:absolute;top:25px;left:25px;font-size:20px;color:#fa5169;font-weight:bold}
.acc.faq+.btnArea{padding:25px 0;}

/** button **/
[class*="Btn"] {display:inline-block;min-width:60px;padding:0 15px;border-radius:3px;border:1px solid #fa5169;background:#fa5169;font-size:15px;font-weight:normal;color:#fff;text-align:center}
[class*="Btn"].color {border:1px solid #ccc;background:#fff;color:#777}
[class*="Btn"].color02 {border:1px solid #fa5169;background:#fff;color:#fa5169}
[class*="Btn"]:disabled,
[class*="Btn"].disabled{-webkit-tap-highlight-color:rgba(0,0,0,0);color:rgba(204,204,204,0.8);border: 1px solid #a5a5a5;background:#a5a5a5}
.sBtn {height:35px;line-height:34px}
.mBtn {height:40px;line-height:39px}
.lBtn {width:100%;height:50px;font-size:18px;line-height:49px}
.btnArea{display:table;position:relative;table-layout:fixed;margin-left:-5px;width:calc(100% + 5px);width:-webkit-calc(100% + 5px);text-align:center}
.btnArea span{display:table-cell;padding-left:5px}
.btnArea span>*{display:block;width:100%;min-width:100%;padding:0}
.btnArea span:first-child{}
[class*="prbtn"] {padding-top:40px}
[class*="prbtn"] span:first-child .btn {border-radius:3px 0 0 3px}
[class*="prbtn"] span:last-child .btn {border-radius:0 3px 3px 0}
[class*="prbtn"] span+span {padding-left:0}
[class*="prbtn"] .btn {display:block;height:55px;background:#fb5169;font-size:18px;color:#fff;line-height:54px}
[class*="prbtn"] .btn.color {background:#555}
.prbtn{margin:0;width:100%;padding-bottom:40px}
.prbtn span{padding:0;}
.prbtnFull {padding-top:14px;margin:15px -25px 0;width:calc(100% + 50px);width:-webkit-calc(100% + 50px);table-layout:auto}
.prbtnFull span{padding:0;}
.prbtnFull span .btn{height:60px;line-height:60px;}
.prbtnFull span:first-child .btn,
.prbtnFull span:last-child .btn {border-radius:0}
.prbtnFull:before{content:'';display:block;position:absolute;top:-10px;left:0;width:100%;height:25px;z-index:1;
	background: -moz-linear-gradient(top, rgba(255,255,255,0) 0%, rgba(245,245,245,0.64) 100%); 
	background: -webkit-linear-gradient(top, rgba(255,255,255,0) 0%, rgba(245,245,245,0.64) 100%); 
	background: linear-gradient(to bottom, rgba(255,255,255,0) 0%, rgba(245,245,245,0.64) 100%);
}
.lBtn em {display:inline-block;vertical-align:middle}
.lBtn em:before {content:'';display:inline-block;width:15px;height:15px;vertical-align:-1px;margin:0 5px 0 10px;background:#fa5169 url(../image/common/bg_ico_more.png) 0 0 no-repeat;background-size:15px}

/** table **/
.infoArea .date{margin:40px 0 10px;font-size:14px;line-height:14px;color:#777;text-align:right}
[class*="tbl"] {overflow:hidden;width:100%;border-top:1px solid #222;border-bottom:1px solid #ccc;text-align:left;background:#fff} 
[class*="tbl"] th,
[class*="tbl"] td{padding:20px 0 20px 20px;font-size:16px;line-height:16px}
[class*="tbl"] th{color:#222}
[class*="tbl"] td{color:#555}
[class*="tbl"] thead th,
.tblV tbody th{background:#f5f5f5}
[class*="tbl"] tbody tr+tr th,
[class*="tbl"] tbody tr+tr td {border-top:1px solid #eee}

/** full Popup **/
.fullPop{position:absolute;top:0;left:0;width:100%;height:100%;background:#fff;z-index:9999}
.fullPop h1{position:fixed;padding:0 58px;width:100%;height:60px;background:#fff;line-height:60px;font-size:20px;font-weight:bold;text-align:center;z-index:1;white-space:nowrap;text-overflow:ellipsis;overflow:hidden}
.fullPop .popCon{margin-top:60px;padding:0 25px;overflow-y:auto}
.fullPop .btnClose{position:fixed;top:0;right:0;width:58px;height:60px;background:url(../image/common/btn_popClose.png) 0 0 no-repeat;background-size:58px auto;text-indent:-9999px;z-index:1}

/** 로그인 **/
.loginWrap .header{position:static;width:100%;height:103px}
.loginWrap .header h1{padding-left:83px;font-size:27px;line-height:103px;text-align:left}
.loginWrap .header .btnBack{top:22px;left:15px}
.loginWrap .content {padding:55px 40px 0}
.loginCon h2 img{display:block;width:184px;height:30px;margin:0 auto}
.loginCon .lBtn {margin-top:50px}
.loginInput {padding-top:50px}
.loginInput .activeInput label {top:13px;left:35px;font-size:14px}
.loginInput .activeInput input {height:46px;padding-left:35px}
.loginInput .activeInput:before {content:'';display:block;position:absolute;bottom:13px;left:5px;width:20px;height:20px;background:url(../image/common/bg_ico_login.png) no-repeat 0 0;background-size:20px 40px}
.loginInput .activeInput+.activeInput:before {background-position:0 -20px}
.loginInput .activeInput input[type='password'] {padding-right:60px}
.loginInput .activeInput button {top:14px}
.loginInput .activeInput+.activeInput .delete {right:30px}
.loginInput .activeInput .pwshow {top:15px;width:21px;background-position:0 -155px}
.loginInput .activeInput.action label {top:-7px;font-size:13px}
.loginCheck {padding:20px 5px 0}
.loginCheck span {width:49%}
.loginCheck span label {font-size:14px;color:#777}
.findInfo {overflow:hidden;padding-top:25px;text-align:center}
.findInfo a {position:relative;display:inline-block;padding:0 12px;font-size:14px;color:#222}
.findInfo a+a:before {content:'';position:absolute;top:50%;left:0;width:1px;height:14px;margin-top:-7px;background:#a5a5a5;vertical-align:middle} 
@media screen and (max-width: 320px){
	.loginWrap .content {padding:55px 20px 0}
	.loginCheck span label,
	.loginInput .activeInput label,
	.findInfo a {font-size:13px}
}

/** 약관동의 **/
.terms .subTitdep01{padding:30px 0 0;font-size:18px;line-height:25px}
.termsArea .totalCheck {margin:25px 0 0;padding:20px 25px;border-top:1px solid #d5d5d5}
.termsArea .termsList {border-top:1px solid #d5d5d5}
.termsArea .termsList b {font-weight:normal}
.termsArea .termsList li {background:#f5f5f5;border-bottom:1px solid #d5d5d5}
.termsArea .termsList li a {display:block;position:relative;padding:20px 40px 20px 25px}
.termsArea .termsList a:after {content:'';display:block;position:absolute;top:50%;right:25px;width:8px;height:8px;margin-top:-4px;border:solid #222;border-width:1px 1px 0 0;-webkit-transform:rotate(45deg);transform:rotate(45deg)}
.termsArea .termsList li .subTerms {padding-bottom:10px}
.termsArea .termsList li .subTerms li {border-bottom:none}
.termsArea .termsList li .subTerms li a {padding:11px 40px 11px 58px;font-size:14px;color:#555}
.termsArea .termsList li .subTerms li a:after {border-color:#777}
.termsArea .termsList li .checkFormCon{position:relative;padding:20px 40px 20px 25px;}
.termsArea .termsList li .checkFormCon a{position:absolute;top:0;right:0;width:100%;height:100%;padding:0;text-indent:-9999px;font-size:0;z-index:1}