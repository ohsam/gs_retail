@charset "utf-8";

/** guide **/
[class^="btnT"].active{border-color:#00573f;background:#00573f}
[class^="btnT"].color{border-color:#00573f;color:#00573f}
.btnMore{border-color:#00573f;background:#00573f}
.btnView{color:#00573f;border-color:#00573f}
.checkForm input[type='checkbox']+label:before, .checkForm input[type='radio']+label:before{background-image:url(../image/fresh/bg_form.png)}
.tabArea ul li a[aria-selected="true"]:after,
.tabTog ul li a[aria-selected="true"],
.tgSwich input:checked + label{background-color:#00573f}

/** 사전예약 **/
.icoStar{display:inline-block;width:22px;height:20px;overflow:hidden;margin-right:5px;color:transparent;vertical-align:-4px;background:url(../image/common/bg_star.png) 0 0 no-repeat;background-size:100% auto}
.setStore {display:flex;flex-wrap:wrap;justify-content:space-between;position:relative;padding:20px 25px;border-top:1px solid #e9e9e9}
.setStore .store{font-size:16px;line-height:33px}
.setStore .store .nm{display:inline-block;color:#00573f;font-weight:bold;line-height:1}
.setStore .store a.nm{border-bottom:1px solid #00573f}

.prList>li{margin-top:10px;position:relative}
.prList>li:before{content:'';position:absolute;top:-10px;left:0;width:100%;height:8px;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;background:#f2f2f2}
.prList>li a{display:block}
.prList .pThum{position:relative;padding:25px}
.prList .pThum .timer{position:absolute;top:25px;right:25px;display:inline-block;padding:0 15px;border:1px solid #c0df16;border-radius:15px;font-size:14px;line-height:23px;}
.prList .pThum .thum{margin:0 -25px;text-align:center}
.prList .pThum .cont{margin-top:25px}
.prList .pThum .price{margin-top:16px;font-size:17px;font-weight:bold}
.prList .pThum .price>li{margin-top:7px}
.prList .pThum .price>li:after{content:'';display:block;clear:both}
.prList .pThum .price>li .data{float:right;font-size:22px;line-height:1}
.prList .pThum .price>li .data.sale{color:#00bb31}
.prList .pThum .price>li.cost{margin-bottom:-3px}
.prList .pThum .price>li.cost .data{color:#777;font-size:16px;font-weight:normal}
.prList .pStatus{padding:20px 25px 25px;color:#555;font-size:16px;border-top:1px solid #ddd;background:#fafafa}
.prList .pStatus .time:before{content:'';display:inline-block;width:17px;height:17px;vertical-align:-2px;background:url(../image/fresh/bg_clock.png) 0 0 no-repeat;background-size:100% auto}
.prList .pStatus .event:before{content:'';display:block;margin:25px -25px;border-top:1px dashed #ddd}

.prDetail .prCon{position:relative;padding:0 25px}
.prDetail .prCon .timer{position:absolute;top:25px;right:25px;display:inline-block;padding:0 15px;border:1px solid #c0df16;border-radius:15px;font-size:14px;line-height:23px}
.prDetail .prCon .prRsvImg{margin:0 -25px;padding-top:25px;text-align:center}

.prInfoTxt{font-size:16px;color:#00573f;line-height:23px}
.prInfoTxt em{display:inline-block;margin-right:5px;padding:5px 7px;background:#00573f;font-size:14px;line-height:14px;color:#fff}
.prTit{margin-top:6px;font-size:18px;color:#111;font-weight:normal;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-wrap:break-word;line-height:1.4}
.prDetail .info{padding:25px 0}
.prDetail .info .price .benefit{margin-top:2px;clear:both}
.prDetail .info .price .benefit:after{content:'';display:block;clear:both}
.prDetail .info .price .benefit span{float:left;margin:5px 0 0 5px;padding:0 15px;border-radius:4px;font-size:14px;line-height:30px;color:#222;font-weight:normal}
.prDetail .info .price .benefit .colorG{background:#c0df16}
.prDetail .info .price .benefit .colorY{background:#ffcd00}
.prDetail .info .price:after{content:'';display:block;clear:both}
.prDetail .info .price dt{float:left;font-size:19px;line-height:28px}
.prDetail .info .price dd{float:right;font-size:28px;font-weight:bold;line-height:28px;word-break:break-all}
.prDetail .info .price{padding-top:20px}

.prDetail .advRsv .prInfo{border-top:1px dashed #ddd;font-size:16px}
.prDetail .advRsv .prInfo>li{padding:15px 25px;border-bottom:1px dashed #ddd}
.prDetail .advRsv .prInfo li:after{content:'';display:block;clear:both}
.prDetail .advRsv .prInfo *{vertical-align:top}
.prDetail .advRsv .prInfo li .prInfoTit{float:left;padding-right:10px;width:32%;line-height:30px;color:#222;word-break:break-all;font-weight:normal}
.prDetail .advRsv .prInfo li .prInfoCon{float:left;width:68%;padding-top:4px;line-height:22px;color:#777}
.prDetail .advRsv .prInfo li .prInfoCon .prices{line-height:23px}
.prDetail .advRsv .prInfo li .prInfoCon .prices li{display:table;margin-top:4px}
.prDetail .advRsv .prInfo li .prInfoCon .prices li:first-child{margin-top:0}
.prDetail .advRsv .prInfo li .prInfoCon .prices li span{display:table-cell}
.prDetail .advRsv .prInfo li .prInfoCon .prices li em{display:table-cell;position:relative;padding-left:17px;word-break:keep-all}
.prDetail .advRsv .prInfo li .prInfoCon .prices li em:before{content:'';display:block;position:absolute;top:5px;left:7px;background:#ccc;width:1px;height:10px}
.prDetail .advRsv .prInfo li .prInfoCon .txtPoint{color:#00bb31}
.prLabel{margin-top:-6px}
.prLabel span{float:left;display:inline-block;margin:10px 5px 0 0;padding:7px 10px;font-size:14px;color:#777;border:1px solid #ccc;border-radius:1px;background:#fff;text-align:center;line-height:1}
.prLabel:after{content:'';display:block;clear:both}

/* sns 공유 */
.snsShare{padding:25px 0;text-align:center}
.snsShare button{margin-left:20px;width:40px;height:40px;background:url(../image/common/bg_sns.png) 0 0 no-repeat;background-size:40px auto;text-indent:-9999px}
.snsShare button:first-child{margin-left:0}
.snsShare .kakao{background-position:0 0}
.snsShare .story{background-position:0 -40px}
.snsShare .facebook{background-position:0 -80px}
.snsShare .instagram{background-position:0 -120px}

/* 합계 고정 버튼 fixed btn 영역 */
.btnFixed.prFixed{height:115px}
.btnFixed.prFixed .prTotal{flex-basis:100%;height:55px;background:#fff;padding:17px 25px}
.btnFixed.prFixed .prTotal:after{content:'';display:block;clear:both}
.btnFixed.prFixed .prTotal .total{float:right;font-size:18px;font-weight:bold}

/*수량 증가 감소*/
.quantity:after{content:'';display:block;clear:both}
.quantity{float:left}
.quantity .minus,
.quantity .plus{float:left;width:21px;height:21px;background:url(../image/common/bg_plusminus.png) 0 0 no-repeat;background-size:21px auto}
.quantity .plus{background-position:0 -21px}
.quantity input[type="number"]{float:left;margin:0;padding:0 5px;width:40px;height:21px;font-size:18px;line-height:21px;text-align:center;border:none}

/*수량 텍스트만 있을때*/
.quantity .txt{font-size:18px}
.quantity .txt em{margin-left:6px;color:#00bb31}

/* 나의예약 */
/*나의예약 step and tap*/
.rsvStep{display:table;width:100%;padding:25px 0 21px;background:url(../image/fresh/bg_rsv_step_line.png) 0 54px repeat-x;background-size:2px auto;}
.rsvStep li{position:relative;display:table-cell;}
.rsvStep li a{display:block;padding-top:60px;}
.rsvStep li a:before{content:'';position:absolute;top:0;left:50%;margin-left:-30px;width:60px;height:60px;background:url(../image/fresh/bg_rsv_step.png) no-repeat;background-size:180px auto;}
.rsvStep li a.step01:before{background-position:0 -60px;}
.rsvStep li a.step02:before{background-position:50% -60px;}
.rsvStep li a.step03:before{background-position:100% -60px;}
.rsvStep li a.step01[aria-selected="true"]:before{background-position:0% 0;}
.rsvStep li a.step02[aria-selected="true"]:before{background-position:50% 0;}
.rsvStep li a.step03[aria-selected="true"]:before{background-position:100% 0;}
.rsvStep li a[aria-selected="true"]{color:#00bb31}

.rsvStep li:first-child:after,
.rsvStep li:last-child:after{content:'';position:absolute;top:0;width:1000px;height:60px;background:#fff;}
.rsvStep li:first-child:after{right:50%;margin-right:30px;}
.rsvStep li:last-child:after{left:50%;margin-left:30px;}
.rsvStep li span{display:block;font-size:14px;text-align:center;}
.rsvStep li .txt{margin-top:10px;font-size:14px;line-height:14px;}
.rsvStep li .num{margin-top:7px;font-size:20px;line-height:20px;font-weight:bold;}

/*예약 상품 리스트*/
.rsvList>li{border-bottom:1px solid #ddd;}
.rsvList .rsvStore{position:relative;padding:15px 25px 12px;width:100%;background:url(../image/fresh/btn_rsv_store.png) 100% -12px no-repeat;background-size:40px auto;text-align:left;}
.rsvList .rsvStore[aria-expanded="false"]{background-position:100% 21px;border-bottom:0;}
.rsvList .rsvStore .name{display:inline-block;padding-top:1px;font-size:16px;line-height:1.4;font-weight:bold;}
.rsvList .rsvStore .icoStar+.name{color:#00573f;}
.rsvList .rsvStore+.rsvStoreCon{display:block;}
.rsvList .rsvStore[aria-expanded="false"]+.rsvStoreCon{display:none;}
.rsvList .rsvStore[aria-expanded="true"]+.rsvStoreCon:first-child{border-top:1px dashed #ddd;}

.rsvList .rsvStoreCon .prInfo{position:relative;padding:0 25px;display:block;border-top:1px solid #ddd;}
.rsvList .rsvStoreCon li:first-child .prInfo{border-top:1px dashed #ddd;}
.rsvList .rsvStoreCon .prInfo .img{position:absolute;top:0;left:0;width:145px;height:145px;}
.rsvList .rsvStoreCon .prInfo .info{padding:18px 0 15px 135px;min-height:145px;}
.rsvList .rsvStoreCon .prInfo .info .tit{font-size:16px;color:#111;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-wrap:break-word;line-height:1.4;}
.rsvList .rsvStoreCon .prInfo .info ul{position:relative;}
.rsvList .rsvStoreCon .prInfo .info ul li{padding-top:29px;font-size:17px;line-height:17px;color:#222;font-weight:bold;}
.rsvList .rsvStoreCon .prInfo .info ul li+li{padding-top:5px;}
.rsvList .rsvStoreCon .prInfo .info ul li del{position:absolute;top:9px;left:0;font-size:13px;color:#777;font-weight:normal;}
.rsvList .rsvStoreCon .prInfo .info ul li em{font-size:14px;color:#555;font-weight:normal;}
.rsvList .rsvStoreCon .date{position:relative;margin:0 -25px;padding:13px 25px 13px;background:#fafafa;font-size:14px;color:#777;line-height:1.4;}
.rsvList .rsvStoreCon .date em{display:inline-block;color:#00bb31;}

.rsvList .rsvStoreCon .date.badge{padding:13px 95px 13px 25px;}
.rsvList .rsvStoreCon .date.badge [class*="badge"]{position:absolute;top:50%;right:15px;margin-top:-13px;padding:0 15px;height:26px;line-height:26px;font-size:13px;font-weight:bold;color:#fff;border-radius:20px;}
.rsvList .rsvStoreCon .date.badge .badge01{background:#00bb31;}
.rsvList .rsvStoreCon .date.badge .badge02{background:#777;}
.rsvList .rsvStoreCon .date.badge .badge03{background:#ccc;}


/** 행사전단 **/





/** GS 수퍼소식 **/



/** 앱주문현관결제 **/
/* 배송지 설정 */
.shipSetting {padding:5px 25px 40px}
.shipSetting .inputSel input {margin:0 5px 0 0;width:calc(100% - 135px);width:-webkit-calc(100% - 135px)}
.shipSetting .inputSel [class*="btnT"] {width:125px}
/* 배송매장 설정 */
.storeSelect li {position:relative;padding:23px 25px 25px;border-bottom:1px solid #e9e9e9}
.storeSelect li .checkForm label {font-size:17px}
.storeSelect li .checkForm label em {display:block;padding:0 100px 20px 0;font-weight:bold}
.storeSelect li button{position:absolute;top:15px;right:25px;z-index:3}
/* 상품선택 리스트*/
.tabArea ul li a em {position:absolute;top:15px;right:-2px;border-radius:50%;width:14px;height:14px;padding-top:2px;background:#00573f;font-size:12px;color:#fff;text-align:center}
.inputArea {padding:15px;background:#f2f2f2}
.inputArea .srch {margin-bottom:0}
.inputArea input {padding-left:15px;background:#fff;border-bottom:none;border-radius:5px}
.infoSet {padding:25px 25px 20px}
.infoList {display:-webkit-flex;display:flex;padding-top:20px;width:100%}
.infoList li {flex:1 20%}
.infoList li a {position:relative;display:block;padding:13px 0;background:#f2f2f2;font-size:15px;color:#777;text-align:center;border-radius:5px 0 0 5px}
.infoList li a:after {content:'';display:block;position:absolute;top:50%;right:0;width:1px;height:20px;margin-top:-10px;background:#ddd}
.infoList li+li a {border-radius:0 5px 5px 0}
.infoList li+li a:after {display:none}
.prRist {padding:0 25px}
.prRist ul{overflow:hidden;display:-webkit-flex;-webkit-flex-wrap:wrap;display:flex;flex-wrap:wrap;height:100%;margin:0 -7px}
.prRist ul li {position:relative;flex:0 50%;padding:25px 7px 35px}
.prRist ul li p {overflow:hidden;height:35px;margin:20px 0 14px;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;text-overflow:ellipsis;word-wrap:break-word;line-height:1.3;font-size:16px}
.prRist ul li em {font-size:18px;font-weight:bold}
.prRist ul li img {width:100%}
.prRist ul li button {position:absolute;bottom:25px;right:5px;width:38px;height:44px;background:url(../image/fresh/bg_ico_order.png) 0 0 no-repeat;background-size:38px auto;font-size:0;line-height:0;color:transparent}
/*.prRist ul li button.on {background-position:0 -44px}*/
.prRist ul li button[aria-selected="true"] {background-position:0 -44px}
.orderGuide {display:none;position:fixed;top:50%;left:0;width:100%;padding:0 25px}
.orderGuide p {padding:20px;border-radius:3px;background-color:rgba( 0, 0, 0, 0.7 );font-size:16px;color:#fff;text-align:center}