@charset "utf-8";

/** guide **/
[class^="btnT"].active{border-color:#00573f;background:#00573f}
[class^="btnT"].color{border-color:#00573f;color:#00573f}
.btnMore{border-color:#00573f;background:#00573f}
.btnView{color:#00573f;border-color:#00573f}
.checkForm input[type='checkbox']+label:before, .checkForm input[type='radio']+label:before{background-image:url(../image/fresh/bg_form.png)}
.tabArea ul li a[aria-selected="true"]:after,
.tabTog ul li a[aria-selected="true"],
.tgSwich input:checked + label{background-color:#00573f}

/** 사전예약 **/
.icoStar{display:inline-block;width:22px;height:20px;overflow:hidden;margin-right:5px;color:transparent;vertical-align:-4px;background:url(../image/common/bg_star.png) 0 0 no-repeat;background-size:100% auto}
.setStore {display:flex;flex-wrap:wrap;justify-content:space-between;position:relative;padding:20px 25px;border-top:1px solid #e9e9e9}
.setStore .store{font-size:16px;line-height:33px}
.setStore .store .nm{display:inline-block;color:#00573f;font-weight:bold;line-height:1}
.setStore .store a.nm{border-bottom:1px solid #00573f}

.prRsvImg{position:relative;margin:0 -25px;text-align:center}
.prRsvImg .timer{position:absolute;top:0;right:25px;display:inline-block;padding:0 15px;border:1px solid #c0df16;border-radius:15px;font-size:14px;line-height:23px}
.prRsvImg .finish{position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;background:rgba(255,255,255,.5) url(../image/fresh/bg_finish.png) 50% 50% no-repeat;background-size:120px auto}
.prInfoTxt{font-size:16px;color:#00573f;line-height:23px}
.prInfoTxt em{display:inline-block;margin-right:5px;padding:5px 7px;background:#00573f;border-radius:1px;font-size:14px;line-height:14px;color:#fff}
.prTit{margin-top:6px;font-size:18px;color:#111;font-weight:normal;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-wrap:break-word}
.prLabel{margin-top:-6px}
.prLabel span{float:left;display:inline-block;margin:10px 5px 0 0;padding:7px 10px;font-size:14px;color:#777;border:1px solid #ccc;border-radius:1px;background:#fff;text-align:center;line-height:1}
.prLabel:after{content:'';display:block;clear:both}

.prBoard{position:relative}
.prList>li{margin-top:10px;position:relative}
.prList>li:before{content:'';position:absolute;top:-10px;left:0;width:100%;height:8px;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;background:#f2f2f2}
.prList>li a{display:block}
.prList .pThum{position:relative;padding:25px}
.prList .pThum .cont{margin-top:25px}
.prList .pThum .price{margin-top:16px;font-size:17px;font-weight:bold}
.prList .pThum .price>li{margin-top:7px}
.prList .pThum .price>li:after{content:'';display:block;clear:both}
.prList .pThum .price>li .data{float:right;font-size:22px;line-height:1}
.prList .pThum .price>li .data.sale{color:#00bb31}
.prList .pThum .price>li.cost{margin-bottom:-3px}
.prList .pThum .price>li.cost .data{color:#777;font-size:16px;font-weight:normal}
.prList .pStatus{padding:20px 25px 25px;color:#555;font-size:16px;border-top:1px solid #ddd;background:#fafafa}
.prList .pStatus .time{color:#222}
.prList .pStatus .time:before{content:'';display:inline-block;width:17px;height:17px;margin-right:5px;vertical-align:-3px;background:url(../image/fresh/bg_clock.png) 0 0 no-repeat;background-size:100% auto}
.prList .pStatus .state{float:right;font-size:14px}
.prList .pStatus .bar{display:block;position:relative;height:2px;margin-top:18px;border-radius:2px;background:#ccc}
.prList .pStatus .bar .ing{position:absolute;top:-2px;bottom:-2px;left:0;width:auto;border-radius:6px;background:#00bb31}
.prList .pStatus .detail{margin-top:25px}
.prList .pStatus .detail li+li{margin-top:5px}
.prList .pStatus .detail .data{margin-left:6px}
.prList .pStatus .detail .data em{color:#00bb31}
.prList .pStatus .event{margin-top:20px}
.prList .pStatus .event:before{content:'';display:block;margin:0 -25px 25px;border-top:1px dashed #ddd}

.prDetail .prCon{position:relative;padding:25px}
.prDetail .info .price .benefit{margin-top:2px;clear:both}
.prDetail .info .price .benefit:after{content:'';display:block;clear:both}
.prDetail .info .price .benefit span{float:left;margin:5px 0 0 5px;padding:0 10px;border-radius:4px;font-size:14px;line-height:29px;color:#222;font-weight:normal}
.prDetail .info .price .benefit .colorG{background:#c0df16}
.prDetail .info .price .benefit .colorY{background:#ffcd00}
.prDetail .info .price:after{content:'';display:block;clear:both}
.prDetail .info .price dt{float:left;font-size:19px;line-height:28px}
.prDetail .info .price dd{float:right;font-size:28px;font-weight:bold;line-height:28px;word-break:break-all}
.prDetail .info .price{padding-top:20px}

.prInfo{border-top:1px dashed #ddd;font-size:16px}
.prInfo>li{padding:22px 25px 16px}
.prInfo>li+li{border-top:1px dashed #ddd}
.prInfo li:after{content:'';display:block;clear:both}
.prInfo *{vertical-align:top}
.prInfo li .prInfoTit{float:left;padding-right:10px;width:32%;color:#222;word-break:break-all;font-weight:normal}
.prInfo li .prInfoCon{float:left;width:68%;color:#777}
.prInfo li .prInfoCon .prices{line-height:23px}
.prInfo li .prInfoCon .prices li{display:table;margin-top:4px}
.prInfo li .prInfoCon .prices li:first-child{margin-top:0}
.prInfo li .prInfoCon .prices li span{display:table-cell}
.prInfo li .prInfoCon .prices li em{display:table-cell;position:relative;padding-left:17px;word-break:keep-all}
.prInfo li .prInfoCon .prices li em:before{content:'';display:block;position:absolute;top:5px;left:7px;background:#ccc;width:1px;height:10px}
.prInfo li .prInfoCon .txtPoint{color:#00bb31}
.prInfo+.snsShare{border-top:1px dashed #ddd}
/* sns 공유 */
.snsShare{padding:25px 0;text-align:center}
.snsShare button{margin-left:20px;width:40px;height:40px;background:url(../image/common/bg_sns.png) 0 0 no-repeat;background-size:40px auto;text-indent:-9999px}
.snsShare button:first-child{margin-left:0}
.snsShare .kakao{background-position:0 0}
.snsShare .story{background-position:0 -40px}
.snsShare .facebook{background-position:0 -80px}
.snsShare .instagram{background-position:0 -120px}
/* 합계 고정 버튼 fixed btn 영역 */
.btnFixed.prFixed{height:115px}
.btnFixed.prFixed .prTotal{flex-basis:100%;height:55px;background:#fff;padding:17px 25px}
.btnFixed.prFixed .prTotal:after{content:'';display:block;clear:both}
.btnFixed.prFixed .prTotal .total{float:right;font-size:18px;font-weight:bold}
/* 수량 증가 감소 */
.quantity:after{content:'';display:block;clear:both}
.quantity{float:left}
.quantity .minus,
.quantity .plus{float:left;width:21px;height:21px;background:url(../image/common/bg_plusminus.png) 0 0 no-repeat;background-size:21px auto}
.quantity .plus{background-position:0 -21px}
.quantity input[type="number"]{float:left;margin:0;padding:0 5px;width:40px;height:21px;font-size:18px;line-height:21px;text-align:center;border:none}
/* 수량 텍스트만 있을때 */
.quantity .txt{font-size:18px}
.quantity .txt em{margin-left:6px;color:#00bb31}

/** 나의예약 **/
/*나의예약 step and tab*/
.rsvStep ul{position:relative;display:flex;padding:25px 0 21px;table-layout:fixed}
.rsvStep ul:before{content:'';position:absolute;top:54px;left:0;width:100%;height:1px;background:#ddd;}
.rsvStep li{position:relative;flex:1 20%;word-break:break-all}
.rsvStep li a{display:block;padding-top:60px}
.rsvStep li a:before{content:'';position:absolute;top:0;left:50%;margin-left:-30px;width:60px;height:60px;background-image:url(../image/fresh/bg_rsv_step.png);background-repeat:no-repeat;background-size:auto 120px}
.rsvStep li a.step01:before{background-position:0 -60px}
.rsvStep li a.step02:before{background-position:-60px -60px}
.rsvStep li a.step03:before{background-position:-120px -60px}
.rsvStep li a.step04:before{background-position:-180px -60px}
.rsvStep li a.step01[aria-selected="true"]:before{background-position:0 0}
.rsvStep li a.step02[aria-selected="true"]:before{background-position:-60px 0}
.rsvStep li a.step03[aria-selected="true"]:before{background-position:-120px 0}
.rsvStep li a.step04[aria-selected="true"]:before{background-position:-180px 0}
.rsvStep li a[aria-selected="true"]{color:#00bb31}

.rsvStep li:first-child:after,
.rsvStep li:last-child:after{content:'';position:absolute;top:0;width:1000px;height:60px;background:#fff}
.rsvStep li:first-child:after{right:50%;margin-right:30px}
.rsvStep li:last-child:after{left:50%;margin-left:30px}
.rsvStep li span{display:block;font-size:14px;text-align:center}
.rsvStep li .txt{margin-top:10px;font-size:14px;line-height:14px}
.rsvStep li .num{margin-top:7px;font-size:21px;line-height:21px;font-weight:bold}
/*예약 상품 리스트*/
.rsvList>li{border-bottom:1px solid #ddd}
.rsvList .rsvStore{position:relative;padding:15px 25px 12px;width:100%;background:url(../image/fresh/btn_rsv_store.png) 100% -12px no-repeat;background-size:40px auto;text-align:left}
.rsvList .rsvStore[aria-expanded="false"]{background-position:100% 21px;border-bottom:0}
.rsvList .rsvStore .name{display:inline-block;padding-top:1px;font-size:16px;font-weight:bold}
.rsvList .rsvStore .icoStar+.name{color:#00573f}
.rsvList .rsvStore+.rsvStoreCon{display:block}
.rsvList .rsvStore[aria-expanded="false"]+.rsvStoreCon{display:none}
.rsvList .rsvStore[aria-expanded="true"]+.rsvStoreCon:first-child{border-top:1px dashed #ddd}

.rsvList .rsvStoreCon li a{position:relative;padding:0 25px;display:block;border-top:1px solid #ddd}
.rsvList .rsvStoreCon li:first-child a{border-top:1px dashed #ddd}
.rsvList .rsvStoreCon a .prRsvImg{position:absolute;top:0;left:0;width:145px;height:145px;margin:0}
.rsvList .rsvStoreCon a .info{padding:18px 0 15px 135px;min-height:145px}
.rsvList .rsvStoreCon a .info .tit{font-size:16px;color:#111;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-wrap:break-word;}
.rsvList .rsvStoreCon a .info ul{position:relative}
.rsvList .rsvStoreCon a .info ul li{padding-top:29px;font-size:17px;line-height:17px;color:#222;font-weight:bold}
.rsvList .rsvStoreCon a .info ul li+li{padding-top:5px}
.rsvList .rsvStoreCon a .info ul li del{position:absolute;top:9px;left:0;font-size:13px;color:#777;font-weight:normal}
.rsvList .rsvStoreCon a .info ul li em{font-size:14px;color:#555;font-weight:normal}
.rsvList .rsvStoreCon .date{position:relative;margin:0 -25px;padding:13px 25px 13px;background:#fafafa;font-size:14px;color:#777;}
.rsvList .rsvStoreCon .date em{display:inline-block;color:#00bb31}

.rsvList .rsvStoreCon .date.badge{padding:13px 95px 13px 25px}
.rsvList .rsvStoreCon .date.badge [class*="badge"]{position:absolute;top:50%;right:15px;margin-top:-13px;padding:0 15px;height:26px;line-height:26px;font-size:13px;font-weight:bold;color:#fff;border-radius:20px}
.rsvList .rsvStoreCon .date.badge .badge01{background:#00bb31}
.rsvList .rsvStoreCon .date.badge .badge02{background:#777}
.rsvList .rsvStoreCon .date.badge .badge03{background:#ccc}


/** 행사전단 **/
.eventBoard{position:relative}
.eventList>li{margin-top:10px;position:relative}
.eventList>li:before{content:'';position:absolute;top:-10px;left:0;width:100%;height:8px;border-top:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;background:#f2f2f2}
.eventList .eHead{padding:22px 25px}
.eventList .eHead .tit{font-size:20px;line-height:1.3}
.eventList .eHead .txt{display:block;margin-top:4px}
.eventList .eBody{padding:22px 25px 40px;border-top:1px dashed #ddd}
.eventList .eBody .flyer{position:relative;margin-top:20px}
.eventList .eBody .flyer:before{content:'';position:absolute;top:0;left:0;width:100%;height:100%;border:1px solid #ddd;box-sizing:border-box}

.eventFloat{height:50px}
.eventFlyer{position:fixed;bottom:0;left:0;width:100%;max-height:80%;padding-top:50px;z-index:100}
.eventFlyer .flyerMore{position:absolute;top:0;left:0;width:100%;height:50px;color:#00573f;font-size:18px;font-weight:bold;border-radius:10px 10px 0 0;background:#fff;box-shadow:0 -5px 20px rgba(0,0,0,.25)}
.eventFlyer .flyerMore:after{content:'';display:inline-block;width:20px;height:20px;margin-left:6px;vertical-align:-3px;background:url(../image/fresh/bg_arrow.png) 0 0 no-repeat;background-size:100% auto}
.eventFlyer .flyerView{display:none;position:relative;height:100%;padding:35px 45px;border-top:1px solid #d5d5d5;background:#fff;overflow-x:hidden;overflow-y:auto}
.eventFlyer .flyerView .swipeArea .swipeCont .swiper>li{margin-right:20px}
.eventFlyer .flyerView .swipeArea .swipeCont .swiper>li a{box-shadow:0 10px 15px rgba(0,0,0,.1)}
.eventFlyer .dimmed{opacity:.7;z-index:-1}
.eventFlyer.open .flyerMore:after{background-position:0 -21px}
.eventFlyer.open .flyerView,
.eventFlyer.open .dimmed{display:block}

.imgViewer{overflow:auto;text-align:center}
.imgViewer img{width:auto}

/** GS 수퍼소식 **/



/** 앱주문현관결제 **/
.rsvStep.type02 li a:before{background-image:url(../image/fresh/bg_order_step.png);}

/* 배송지 설정 */
.shipSetting {padding:5px 25px 40px}
.shipSetting .inputSel input {margin:0 5px 0 0;width:calc(100% - 135px);width:-webkit-calc(100% - 135px)}
.shipSetting .inputSel [class*="btnT"] {width:125px}
/* 배송매장 설정 */
.storeSelect li {position:relative;padding:25px 25px 19px;border-bottom:1px solid #e9e9e9}
.storeSelect li .checkForm label {font-size:17px}
.storeSelect li .checkForm label em {display:block;padding:0 100px 20px 0;font-weight:bold}
.storeSelect li button{position:absolute;top:15px;right:25px;z-index:3}
/* 배송지 관리 */
.prInfo a{float:left;margin:10px 5px 0 0}
.storeSubTit{padding:25px;font-size:18px}
.storeSubTit,
.storeSubTit+.storeSelect li:last-child{border-bottom:1px solid #ddd}
.storeSubTit+.storeSelect li{border-bottom:1px solid #eee}
/* 상품선택 리스트*/
.tabArea ul li a em {position:absolute;top:15px;right:-2px;border-radius:50%;width:14px;height:14px;padding-top:2px;background:#00573f;font-size:12px;color:#fff;text-align:center}
.infoSet {padding:25px 25px 20px}
.infoList {display:-webkit-flex;display:flex;padding-top:20px;width:100%}
.infoList li {flex:1 20%}
.infoList li a {position:relative;display:block;padding:13px 0;background:#f2f2f2;font-size:15px;color:#777;text-align:center;border-radius:5px 0 0 5px}
.infoList li a:after {content:'';display:block;position:absolute;top:50%;right:0;width:1px;height:20px;margin-top:-10px;background:#ddd}
.infoList li+li a {border-radius:0 5px 5px 0}
.infoList li+li a:after {display:none}
.prRist {padding:0 25px}
.prRist ul{overflow:hidden;display:-webkit-flex;-webkit-flex-wrap:wrap;display:flex;flex-wrap:wrap;height:100%;margin:0 -7px}
.prRist ul li {position:relative;flex:0 50%;padding:25px 7px 35px}
.prRist ul li p {overflow:hidden;height:35px;margin:20px 0 14px;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;text-overflow:ellipsis;word-wrap:break-word;line-height:1.3;font-size:16px}
.prRist ul li em {font-size:18px;font-weight:bold}
.prRist ul li img {width:100%}
.prRist ul li button {position:absolute;bottom:25px;right:5px;width:38px;height:44px;background:url(../image/fresh/bg_ico_order.png) 0 0 no-repeat;background-size:38px auto;font-size:0;line-height:0;color:transparent}
/*.prRist ul li button.on {background-position:0 -44px}*/
.prRist ul li button[aria-selected="true"] {background-position:0 -44px}
.orderGuide {display:none;position:fixed;top:50%;left:0;width:100%;padding:0 25px}
.orderGuide p {padding:20px;border-radius:3px;background-color:rgba( 0, 0, 0, 0.7 );font-size:16px;color:#fff;text-align:center}
/* 이용안내 */
.svGuide h2{padding:30px 0 25px;text-align:center}
.svGuide ul{display:flex;text-align:center}
.svGuide ul li{position:relative;flex:1 20%;width:70px;padding:80px 0 40px;font-size:14px}
.svGuide ul li:before{content:'';position:absolute;top:0;left:50%;margin-left:-35px;width:70px;height:70px;background:url(../image/fresh/bg_order_guide_step.png) no-repeat;background-size:70px auto}
.svGuide ul li.step01:before{background-position:0 0}
.svGuide ul li.step02:before{background-position:0 -70px}
.svGuide ul li.step03:before{background-position:0 -140px}
.svGuide ul li:after{content:'';position:absolute;top:33px;right:-12px;width:23px;height:3px;background:url(../image/fresh/bg_order_guide_step_dotted.png) no-repeat;background-size:auto 3px}
.svGuide ul li:last-child:after{display:none}

/* 주문하기 */
.odInfo .titArea{padding:15px 25px;border-bottom:1px solid #ddd;overflow:hidden}
.odInfo .titArea .title{font-size:18px;line-height:33px}
.odInfo .titArea a{position:absolute;right:25px;top:15px}
.odInfo .odPerson{padding:0 25px 40px}
.odInfo .odPerson .tit:first-child{padding:30px 0 0}
.odInfo .odPerson .tit{display:block;padding:40px 0 0;font-size:18px}
.odInfo .odPerson ul li:first-child .activeInput{margin-top:15px}

.odList .title{padding:18px 25px;font-size:18px}
.odList .odListCon li{position:relative;padding:0 25px;display:block;border-top:none}
.odList .odListCon{border-top:1px solid #ddd;border-bottom:1px solid #ddd}
.odList .odListCon li+li{border-top:1px solid #eee}
.odList .odListCon li .prRsvImg{position:absolute;top:0;left:0;width:145px;height:145px;margin:0}
.odList .odListCon li .info{padding:18px 0 15px 135px;min-height:145px}
.odList .odListCon li .info .tit{font-size:16px;color:#111;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-wrap:break-word}
.odList .odListCon li .info .price{display:block;padding-top:15px;font-size:17px;color:#222;font-weight:bold}
.odList .odListCon .control .quantity{position:absolute;bottom:15px;left:160px}
.odList .odListCon .control .odDel{position:absolute;bottom:0;right:0;width:72px;height:52px;background:url(../image/fresh/btn_od_del.png) 50% 50% no-repeat;background-size:21px auto}
.odList .odTotal{padding:23px 25px;line-height:18px;overflow:hidden}
.odList .odTotal .txt{float:left;padding-top:1px;width:50%;font-size:16px}
.odList .odTotal .price{float:right;width:50%;font-size:18px;text-align:right}
/* 주문내역 */
.odListDetail{font-size:16px}
.odListDetail *{vertical-align:top}
.odListDetail li{padding:12px 25px 0}
.odListDetail li:after{content:'';display:block;clear:both}
.odListDetail li .odTit{float:left;padding-right:10px;width:40%;color:#222;word-break:break-all;font-weight:normal}
.odListDetail li .odCon{float:left;width:60%;color:#777}
.odListDetail li:first-child{padding-top:23px}
.odListDetail li:last-child{padding-bottom:19px}
.odListDetail .topBder{margin-top:19px;padding-top:23px;border-top:1px dashed #ddd}

.odReceipt{padding:25px 25px 40px;background:#f2f2f2}
.odReceipt:before{content:'';display:block;position:relative;height:5px;background:url(../image/fresh/bg_table_edge.png) 0 -5px repeat-x;background-size:auto 10px}
.odReceipt:after{content:'';display:block;position:relative;height:5px;background:url(../image/fresh/bg_table_edge.png) 0 0 repeat-x;background-size:auto 10px}

.odReceiptTb{padding:25px 17px 31px;background:#fff;font-size:16px;word-break:break-all}
.odReceiptTb thead th{padding-bottom:15px}
.odReceiptTb tbody{border-top:1px solid #222}
.odReceiptTb tbody tr td{padding:15px 0}
.odReceiptTb tbody tr+tr{border-top:1px solid #ddd}
.odReceiptTb tfoot{border-top:1px solid #222}
.odReceiptTb .name{text-align:left}
.odReceiptTb .name span{overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-wrap:break-word}
.odReceiptTb .num{text-align:center}
.odReceiptTb .price{text-align:right}
.odReceiptTb tfoot .txt{padding-top:26px;text-align:left}
.odReceiptTb tfoot .total{padding-top:26px;text-align:right;font-weight:bold}
/* 배송현황 */
.orderList li+li{border-top:1px solid #eee}
.orderList li a{position:relative;display:block;padding:25px}
.orderList [class*="od"]{display:block;font-size:14px;color:#222;word-break:break-all}
.orderList .odShop{padding-right:80px;font-size:16px;font-weight:bold}
.orderList .odDate{padding-top:8px}
.orderList .odName{padding-top:6px;width:55%;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;}
