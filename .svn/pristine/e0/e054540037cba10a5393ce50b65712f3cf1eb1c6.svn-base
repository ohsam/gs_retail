.creditCard_select {padding-top:25px}
.saleInfo .saleDetail {padding-left:33px}
.saleDetail span{position:relative;overflow:hidden;clear:both;display:block;margin-bottom:13px;padding:0 50px 0 15px;font-size:14px;color:#777}
.saleDetail span em {position:absolute;top:0;right:0}
.saleDetail span:before {content:'';position:absolute;top:5px;left:0;width:6px;height:6px;vertical-align:3px;border:solid #ccc;border-width:1px 1px 0 0;background:none;-webkit-transform:rotate(180deg);transform:rotate(180deg)}
.mobilePop {padding:20px 25px 25px}
.mobilePop .introTxt {margin-bottom:25px;padding-bottom:25px;border-bottom:1px dashed #ddd;text-align:center}
.mobilePop .introTxt p {padding:0 42px 120px;background:url(../image/common/bg_ico_mobilepop.png) 50% bottom no-repeat;background-size:145px 109px}
.mobilePop .introTxt p em {display:block;padding-bottom:10px;font-size:14px;color:#777}
.mobilePop .introTxt p strong {font-size:16px}
.mobilePop .introTxt a {margin-top:23px}
.mobilePop .saleInfo .saleDetail {padding-left:0}
.mobilePop .cashSelect,
.bankTransfer .cashSelect{margin:25px 0}
.mobilePop .popCharging {position:relative;display:block;padding-bottom:40px}
.mobilePop .popCharging a {position:absolute;top:-5px;right:0}
/* 휴대폰결제 */
.phonePayment,
.bankTransfer{padding:5px 25px 25px}
/* 실시간계좌이체 */
.bankTransfer .introTxt p {padding:15px 0} 
/*최종 결제 금액*/
.lastPayList{word-break:break-all;overflow:hidden}
.lastPayList>li{position:relative;margin-top:25px;padding:0 25px;font-size:16px;/*border-top:none;*/}
.lastPayList>li:last-child{margin-bottom:25px;overflow:initial !important}
.lastPayList>li:last-child .accCon{margin-bottom:-25px}
.lastPayList li:after{content:'';display:block;clear:both}
.lastPayList li strong,
.lastPayList li.acc .accTit strong{position:relative;float:left;font-weight:400;font-size:16px}
.lastPayList li .price,
.lastPayList li.acc .accTit .price{float:right;font-weight:500;position:static;transform:none;-webkit-transform:none}
.lastPayList li.acc .accTit button{width:100%;padding:10px 0;margin:-10px 0}
.lastPayList li.acc .accTit button:after{display:none}
/*.lastPayList li.acc .accTit button[aria-selected="true"] strong:after{content:'';display:block;position:absolute;top:50%;right:-21px;width:8px;height:8px;margin-top:-2px;border:solid #222;border-width:1px 1px 0 0;-webkit-transform:rotate(-45deg);transform:rotate(-45deg)}*/
.lastPayList li.acc .accTit button strong:after{content:'';display:block;position:absolute;top:50%;right:-21px;width:8px;height:8px;margin-top:-8px;border:solid #222;border-width:1px 1px 0 0;-webkit-transform:rotate(135deg);transform:rotate(135deg)}
.lastPayList li.acc .accTit button[aria-selected="true"] strong:after{margin-top:-2px;-webkit-transform:rotate(-45deg);transform:rotate(-45deg)}
.lastPayList li .checkForm{display:block;padding-top:20px;clear:both}
.lastPayList li.acc .accCon{margin:25px -25px 0;padding:25px 25px 13px;background:#fafafa;color:#777;border-top:none}
.paymentWrap .acc .accCon .termsArea .acc .accCon{background:#f2f2f2}
.lastPay .lastPayList .accTit strong:before{display:none}
/* 결제/인증 */
.infoData.totalData li>span{text-align:right}
.infoData.totalData li.total{padding-top:21px;margin-top:20px;border-top:2px solid #222;color:#222}
.infoData.totalData li.total>strong{margin-top:2px;font-size:16px;color:#222;font-weight:bold}
.infoData.totalData li.total>span{font-size:18px;color:#222;font-weight:bold}
.cardListArea{padding:30px 25px}
.cardListArea .cardList+select{margin-top:25px}
.cardList{display:flex;flex-wrap:wrap}
.cardList .checkForm{flex:0 25%;text-align:center}
.cardList .checkForm input[type='radio']+label{display:block;padding:54px 0 16px;color:#777}
.cardList .checkForm input[type='radio']+label:before{content:'';position:absolute;top:1px;left:50%;width:50px;height:50px;margin-left:-25px;background:url(../image/common/bg_ico_card.png) 0 0 no-repeat;background-size:50px auto}
.cardList .checkForm input[type='radio']:checked+label{border:1px solid #fa5169;border-radius:2px;padding:53px 0 15px;color:#fa5169}
.cardList .checkForm input[type='radio']:checked+label:before{top:0}
.cardList .checkForm.shinhan input[type='radio']+label:before{background-position:0 0}
.cardList .checkForm.hana input[type='radio']+label:before{background-position:0 -54px}
.cardList .checkForm.bc input[type='radio']+label:before{background-position:0 -108px}
.cardList .checkForm.kb input[type='radio']+label:before{background-position:0 -162px}
.cardList .checkForm.samsung input[type='radio']+label:before{background-position:0 -216px}
.cardList .checkForm.hyundai input[type='radio']+label:before{background-position:0 -270px}
.cardList .checkForm.nh input[type='radio']+label:before{background-position:0 -324px}
.cardList .checkForm.keb input[type='radio']+label:before{background-position:0 -378px}
.cardList .checkForm.lotte input[type='radio']+label:before{background-position:0 -432px}
.cardList .checkForm.citi input[type='radio']+label:before{background-position:0 -486px}
.cardList .checkForm.woori input[type='radio']+label:before{background-position:0 -540px}
.cardList .checkForm.gwangju input[type='radio']+label:before{background-position:0 -594px}
.cardList .checkForm.jeju input[type='radio']+label:before{background-position:0 -648px}
.cardList .checkForm.suhyup input[type='radio']+label:before{background-position:0 -702px}
.cardList .checkForm.jeonbuk input[type='radio']+label:before{background-position:0 -756px}
.cardList .checkForm.postcheck input[type='radio']+label:before{background-position:0 -810px}
.install.ispApp p:before{background:url(../image/common/bg_isp_app.png) 0 0 no-repeat;background-size:40px auto}
.install{padding:30px 25px 25px 82px;border-top:1px solid #ddd;font-size:16px}
.install p{position:relative}
.install p:before{position:absolute;top:0;left:-59px;content:'';width:40px;height:40px}
.install a{display:inline-block;margin-top:7px;color:#fa5169;text-decoration:underline}




