.swipeArea{position:relative;overflow:hidden}
.swipeArea .swipeCont{position:relative}
.swipeArea .swipeCont .swiper{display:flex;flex-wrap:nowrap;align-items:center}
.swipeArea .swipeCont .swiper>li{flex:1 0 100%}
.swipeArea .swipeCont .swiper>li a{display:block}
.swipeArea .indicator{display:block;margin-top:25px;font-size:0;text-align:center}
.swipeArea .indicator button{display:inline-block;width:13px;height:13px;padding:4px;margin:-4px 0;color:transparent;font-size:13px;overflow:hidden}
.swipeArea .indicator button:before{content:'';display:block;height:100%;border-radius:5px;background:#777}
.swipeArea .indicator button[aria-selected="true"]{width:26px}
.swipeArea .swipeCtrl .indicator{display:inline-block;margin:0;vertical-align:middle}
.swipeArea .swipeCtrl .btnCtrl{width:16px;height:18px;margin:-2px 0 0 4px;padding:4px;color:transparent;font-size:13px;vertical-align:middle;text-indent:-9999px;overflow:hidden}
.swipeArea .swipeCtrl .btnCtrl:before{content:'';display:block;height:100%;overflow:hidden;background:url(../image/common/btn_swipe.png) 0 0 no-repeat;background-size:100% auto}
.swipeArea .swipeCtrl .btnCtrl.stop:before{background-position:0 0}
.swipeArea .swipeCtrl .btnCtrl.play:before{background-position:0 -10px}

