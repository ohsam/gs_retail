.storeViewer .viewCtrl .ctrl{display:none;position:fixed;bottom:25px;left:50%;margin-left:-40px;width:80px;color:#fa5169;font-size:15px;line-height:33px;border-radius:35px;border:1px solid #fa5169;background:#fff;box-shadow:5px 5px 10px rgba(0,0,0,.1);z-index:2;animation-duration:1s}
