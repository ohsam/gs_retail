.bnfPast .bnfGraph a:after{content:'';position:absolute;top:50%;right:22px;width:9px;height:9px;margin-top:-7px;border:solid #222;border-width:1px 1px 0 0;border-radius:1px;transform:rotate(45deg)}
.bnfPast .bnfGraph .grp{float:left;position:relative;width:80px;height:80px;margin:-13px 13px 0 -7px}
.bnfPast .bnfGraph .grp .spot{display:none;position:absolute;top:50%;left:50%;width:14px;height:66px;margin:-33px 0 0 -7px;transform:rotate(0)}
.bnfPast .bnfGraph .grp .spot:before{content:'';position:absolute;top:0;left:0;width:10px;height:10px;border:2px solid #007cff;border-radius:100%;background:#fff}
.bnfPast .bnfGraph .con{overflow:hidden}
.bnfPast .bnfGraph .con .tit{font-size:16px}
.bnfPast .bnfGraph .con .txt{margin-top:10px;font-size:17px}
.bnfPast .bnfGraph .con .txt .total{margin-left:4px;color:#777}

.bnfTotal{padding:21px 25px;border-bottom:1px solid #e9e9e9}
.bnfTotal .total{font-size:17px;font-weight:500}
.bnfTotal .total:before{content:'';display:inline-block;width:21px;height:20px;margin:0 10px 0 0;vertical-align:middle;background:url(../image/common/bg_bnf_price.png) 0 0 no-repeat;background-size:100% auto}
.bnfTotal .total .num{float:right;font-size:22px;line-height:1}

.bnfPrize .prizeSort{padding:23px 25px 21px;border-top:1px solid #ddd;border-bottom:1px dashed #ddd}
.bnfPrize .prizeSort:first-child{border-top:0}
.bnfPrize .prizeSort .sort{font-size:18px;line-height:1.2}
.bnfPrize .prizeList{border-top:0}

.priceHistory{position:relative;margin-bottom:-1px;border-top:1px solid #e9e9e9}
.priceHistory li{position:relative;border-bottom:1px solid #ddd;padding:22px 25px}
.priceHistory li a{position:relative;display:block;margin:-22px -25px;padding:22px 110px 22px 25px}
.priceHistory li a:after{content:'';position:absolute;top:50%;right:27px;width:9px;height:9px;margin-top:-5px;border:solid #777;border-width:1px 1px 0 0;border-radius:1px;transform:rotate(45deg)}
.priceHistory .tit{display:block;font-size:16px;font-weight:400}
.priceHistory .txt{display:block;color:#777;padding-top:8px}
.priceHistory .price{position:absolute;top:50%;right:25px;transform:translateY(-50%);font-size:16px;font-weight:500}
.priceHistory a .price{right:43px}

/** 보관함 **/
.prRsvImg .endPeriod{position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;background:rgba(255,255,255,.5) url(../image/gs25/bg_end_period.png) 50% 50% no-repeat;background-size:120px auto;z-index:1}
.prRsvImg .endUse{position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;background:rgba(255,255,255,.5) url(../image/gs25/bg_end_use.png) 50% 50% no-repeat;background-size:120px auto;z-index:1}
.prRsvImg .sendGift{position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;background:rgba(255,255,255,.5) url(../image/gs25/bg_send_gift.png) 50% 50% no-repeat;background-size:120px auto;z-index:1}

/* 보관함-상품변경하기(P) */
.prChangeCon .checkForm input[type='radio']+label{padding:0}

/* 보관함 목록 */
.totalCon{padding:30px 25px}
.totalCon .txt{text-align:center;font-size:18px}
.totalCon .total{display:flex;flex-wrap:wrap;margin-top:18px;border:1px solid #ddd;border-radius:35px}
.totalCon .total span{flex:1 auto;margin:10px 0;text-align:center;line-height:15px}
.totalCon .total span strong{margin-left:10px;font-size:15px;vertical-align:-1px}
.totalCon .total span+span{border-left:1px solid #ddd}

.prRist.storage .date{display:block;margin-top:13px;color:#777}
.prRist.storage ul li {position:relative}
.prRist.storage ul li a.conBtn {display:block;padding:0 8px 35px}
.prRist.storage ul li a.giftBtn{position:absolute;bottom:-5px;right:3px;width: 28px;height: 28px;padding: 20px;background: url(../image/gs25/bg_ico_gift.png) 50% 50% no-repeat;background-size: 28px auto;font-size:0;line-height:0;color:transparent}
.prRist.storage ul li .img{position:relative}
.prRist.storage ul li .bgTxt{position:absolute;top:0;left:0;width:100%;height:100%;color:transparent;background:rgba(255,255,255,.5)}
.prRist.storage ul li .txt{position:absolute;top:50%;left:50%;width:96px;height:96px;color:#fff;margin:-48px 0 0 -48px;padding:10px;text-align:center;font-size:13px;box-shadow:15px 15px 25px rgba(0,0,0,.2);border-radius:50%;display:flex;flex-direction:column;justify-content:center;background:rgba(0,0,0,.7)}
.prRist.storage ul li .type{display:block;padding:2px 12px 0;position:absolute;bottom:4px;left:8px;background:#fff;border:1px solid #007cff;color:#007cff;border-radius:15px}
.prRist.storage ul li .dday{position:absolute;top:0;right:0}
.prRist.storage ul li .dday,
.prViewArea .dday{display:flex;justify-content:center;align-items:center;width:46px;height:46px;background:#ff6a13;color:#fff;border-radius:50% 50% 50% 0;text-align:center;font-size:12px;line-height:14px;padding:2px 5px}
.content.gs25 .prRist.storage ul li+li+li{margin-top:60px}

/* 보관 상품 교환 방법 */
